{
	"singleQuote": false,
	"semi": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"bracketSameLine": false,
	"svelteSortOrder": "options-scripts-styles-markup",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"]
}
